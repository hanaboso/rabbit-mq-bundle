{
    "name": "hanaboso/rabbit-mq-bundle",
    "description": "Hanaboso - rabbit-mq-bundle",
    "license": "Apache-2.0",
    "version": "1.9.0",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/promises": "^2.2",
        "hanaboso/utils": "^1.6",
        "php-amqplib/php-amqplib": "^3.7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "RabbitMqBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RabbitBundleTests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
