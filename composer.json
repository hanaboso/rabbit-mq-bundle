{
    "name": "hanaboso/rabbit-mq-bundle",
    "description": "Hanaboso - rabbit-mq-bundle",
    "license": "Apache-2.0",
    "version": "1.8.0",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/promises": "^1.5",
        "hanaboso/utils": "^1.5",
        "php-amqplib/php-amqplib": "^3.6",
        "symfony/monolog-bundle": "^3.10",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "RabbitMqBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RabbitBundleTests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
